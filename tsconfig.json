{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext", 
     "outDir": "./dist",        
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "noUnusedLocals": true ,
    "noUnusedParameters": true ,
    "noImplicitReturns": true ,
    "moduleResolution": "node",
    "skipLibCheck": true
  }
}
